<%= render partial: 'layout' %>
<div class="logs">
  
  <h2>Api Log</h2>
    <form class="madals">
      <div class="form-group">
        <label for="company_name">Company Name:</label>
        <p id="api_company_name" type="text" class="form-control" disabled ><%= @log.company_name%></p>
      </div>
      <div class="form-group">
        <label for="company_domain">Company Domain:</label>
        <p id="api_company_domain" type="text" class="form-control" disabled ><%= @log.company_domain%></p>
      </div>
      <div class="form-group">
        <label for="data_received">Data Received:</label>
        <p id="api_data_received" type="text" class="form-control" disabled ><%= @log.data_received%></p>
      </div>
      <div class="form-group">
        <label for="end_point">End Point:</label>
        <p id="api_end_point" type="text" class="form-control" disabled><%= @log.end_point%></p>
      </div>
      <div class="form-group">
        <label for="location">Location:</label>
        <p id="api_location" type="text" class="form-control" disabled><%= @log.location%></p>
      </div>
      <div class="form-group">
        <label for="message">Message:</label>
        <p id="api_message" type="text" class="form-control" disabled><%= @log.message%></p>
      </div>


      <div class="form-group">
        <label for="status">Status:</label>
        <p id="api_status" type="text" class="form-control" disabled><%= @log.status%></p>
      </div>

      <div class="form-group">
        <label for="created_at">Created At:</label>
        <p id="api_created_at" type="text" class="form-control" disabled><%= @set_date %></p>
      </div>
    </form>
  </div>
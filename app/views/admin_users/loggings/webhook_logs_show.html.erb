<%= render partial: 'layout' %> 
  <div class="logs">
    
    <h2>Webhook Log </h2>
    <form class="madals">
      <div class="form-group">
        <label for="company_name">Company Name:</label>
        <p id="webhook_company_name" type="text" class="form-control" disabled ><%= @log.company_name%></p>
      </div>
      <div class="form-group">
        <label for="company_domain">Company Domain:</label>
        <p id="webhook_company_domain" type="text" class="form-control" disabled><%= @log.company_domain%></p>
      </div>
      <div class="form-group">
        <label for="integration">Integration:</label>
        <p id="webhook_integration" type="text" class="form-control" disabled><%= @log.integration%></p>
      </div>
      <div class="form-group">
        <label for="data_received">Data Received:</label>
        <p id="webhook_data_received" type="text" class="form-control" disabled><%= @log.data_received%></p>
      </div>
      <div class="form-group">
        <label for="location">Location:</label>
        <p id="webhook_location" type="text" class="form-control" disabled><%= @log.location%></p>
      </div>
      <div class="form-group">
        <label for="error_message">Error Message:</label>
        <p id="webhook_error_message" type="text" class="form-control" disabled><%= @log.error_message%></p>
      </div>
      <div class="form-group">
        <label for="action">Action:</label>
        <p id="webhook_action" type="text" class="form-control" disabled><%= @log.action%></p>
      </div>

      <div class="form-group">
        <label for="status">Status:</label>
        <p id="webhook_status" type="text" class="form-control" disabled><%= @log.status%></p>
      </div>

      <div class="form-group">
        <label for="created_at">Created At:</label>
        <p id="webhook_created_at" type="text" class="form-control" disabled><%= @set_date %></p>
      </div>
    </form>

  </div>
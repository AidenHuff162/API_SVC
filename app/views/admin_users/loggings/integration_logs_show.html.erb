<%= render partial: 'layout' %>
<div class="logs">
  <h2>Integration Log </h2>
  <form class="madals">
    <div class="form-group">
      <label for="company_name">Company Name:</label>
      <p id="integ_company_name" type="text" class="form-control" disabled><%= @log.company_name%></p>
    </div>
    <div class="form-group">
      <label for="company_domain">Company Domain:</label>
      <p id="integ_company_domain" type="text" class="form-control" disabled><%= @log.company_domain%></p>
    </div>
    <div class="form-group">
      <label for="integration">Integration:</label>
      <p id="integ_integration" type="text" class="form-control" disabled ><%= @log.integration%></p>
    </div>
    <div class="form-group">
      <label for="action">Action:</label>
      <p id="integ_action" type="text" class="form-control" disabled><%= @log.action%></p>
    </div>
    <div class="form-group">
      <label for="request">Request:</label>
      <p id="integ_request" type="text"  class="form-control" disabled><%= @log.request%></p>
    </div>

    <div class="form-group">
      <label for="response">Response:</label>
      <p id="integ_response" type="text" class="form-control" disabled><%= @log.response%></p>
    </div>

    <div class="form-group">
      <label for="status">Status:</label>
      <p id="integ_status" type="text" class="form-control" disabled><%= @log.status%></p>
    </div>

    <div class="form-group">
      <label for="created_at">Created At:</label>
      <p id="integ_created_at" type="text" class="form-control" disabled><%= @set_date %></p>
    </div>
  </form>
</div>
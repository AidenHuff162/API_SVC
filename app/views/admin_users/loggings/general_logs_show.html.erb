<%= render partial: 'layout' %> 
  <div class="logs">
    <h2><%= @log.log_type %> Log</h2>    
    <form class="madals">
      <div class="form-group">
        <label for="company_name">Company Name:</label>
        <p id="pto_company_name" type="text" class="form-control" disabled ><%= @log.company_name%></p>
      </div>
      <div class="form-group">
        <label for="company_domain">Company Domain:</label>
        <p id="pto_company_domain" type="text" class="form-control" disabled ><%= @log.company_domain%></p>
      </div>
      <div class="form-group">
        <label for="action">Action:</label>
        <p id="pto_action" type="text" class="form-control" disabled><%= @log.action%></p>
      </div>
      <div class="form-group">
        <label for="result">Result:</label>
        <p id="pto_result" type="text" class="form-control" disabled><%= @log.result%></p>
      </div>

      <div class="form-group">
        <label for="created_at">Created At:</label>
        <p id="pto_created_at" type="text" class="form-control" disabled><%= @set_date%></p>
      </div>
    </form>
  </div>